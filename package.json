{
  "name": "clean-arch-ts-pokemon",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "husky install",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "build:watch": "rimraf dist && tsc -p tsconfig-build.json -w",
    "start": "node dist/main/server.js",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit.config.js",
    "test:integration": "npm test -- --watch -c jest-integration.config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "lint:staged": "eslint 'src/**' --fix",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akappes/clean-arch-ts-pokemon.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/akappes/clean-arch-ts-pokemon/issues"
  },
  "homepage": "https://github.com/akappes/clean-arch-ts-pokemon#readme",
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/node": "^8.0.29",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "eslint": "^7.19.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "husky": "^5.0.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "sucrase": "^3.17.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.0",
    "ts-node": "3.3.0",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mssql": "^6.3.1",
    "reflect-metadata": "^0.1.10",
    "sequelize": "^6.5.1"
  },
  "_moduleAliases": {
    "@": "dist",
    "@tests": "__tests__",
    "@fixtures": "__fixtures__"
  }
